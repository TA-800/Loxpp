var i = 0;
print "Starting loop 1";
while (i < 3) {

    print i;

    print "Starting loop 2";
    var j = 0;
    while (j < 3) {

        print j;

        print "Starting loop 3";
        for (var k = 0; k < 3; k = k + 1) {

            print k;

            if (k == 1) {
                break;
            }
        }

        break;
    }

    break;

}

